<odoo>
    <record id="action_locasix_quote" model="ir.actions.report">
        <field name="name">Offre de prix Locasix</field>
        <field name="model">sale.order</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">locasix.locasix_quote_report_document</field>
        <field name="report_file">locasix.locasix_quote_report_document</field>
        <field name="print_report_name">'Offre de prix - %s' % (object.name) </field>
        <field name="binding_model_id" ref="model_sale_order"/>
        <field name="binding_type">report</field>
        <field name="paperformat_id" ref="locasix.paperformat_quote"/>
    </record>
</odoo>