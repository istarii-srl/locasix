<odoo>
    <template id="locasix_quote_report_document">
        <t t-call="web.basic_layout">
            <div class="page">
                <div class="row" style="padding: 0px !important; margin: 0px !important">
                    <div class="col-3" style="background-color: blue; padding: 0px !important; margin: 0px !important">
                        <img src="/locasix/static/img/locasix_lateral_bar.png" class="img-fluid"/>
                    </div>
                    <div class="col-9" style="background-color: red;">
                        <span>YO</span>
                    </div>
                </div>
            </div>
        </t>
    </template>

    <template id="locasix_quote_report_2">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="locasix.locasix_quote_report_document" t-lang="doc.partner_id.lang"/>
            </t>
        </t>
    </template>
</odoo>