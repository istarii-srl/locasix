<odoo>
    <template id="locasix.locasix_quote_report_document">
        <t t-call="web.basic_layout">
            <div class="row" style="padding: 35px 15px 35px 15px !important; margin: 0px !important">
                <div class="col" style="width: 235px !important; height: 1245px !important; padding: 0px !important;">
                    <img src="/locasix/static/img/locasix_lateral_bar.png" style="object-fit: contain"/>
                </div>
                <div class="col" style="width: 635px !important; height: 1245px !important; padding-left: 35px !important; background-color: blue;">
                    <div style="height: 65px;"/>
                    <div style="height: 4px; width: 40px; background-color: red"/>
                    <h2>OFFRE DE <br/>PRIX</h2>
                    <div style="height: 100px;"/>
                    <div class="row" style="padding: 0px !important;">
                        <div style="width: 295px; border: 1px;">
                            <span>YO</span>
                        </div>
                        <div style="width: 1px;"/>
                        <div style="width: 295px; border: 1px;">
                            <span>YO</span>
                        </div>
                    </div>
                    
                </div>
            </div>
        </t>
    </template>

    <template id="locasix_quote_report_2">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="locasix.locasix_quote_report_document" t-lang="doc.partner_id.lang"/>
            </t>
        </t>
    </template>
</odoo>