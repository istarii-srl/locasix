<odoo>
    <template id="locasix.locasix_quote_report_document">
        <t t-call="web.basic_layout">
        <div class="row" style="padding: 35px 15px 35px 15px !important; margin: 0px !important">
            <div class="col" style="width: 235px !important; height: 1245px !important; background-color: blue; padding: 0px !important;">
                <img src="/locasix/static/img/locasix_lateral_bar.png" style="object-fit: contain"/>
            </div>
            <div class="col" style="width: 635px !important; height: 1245px !important; background-color: red;">
                <span>YO</span>
            </div>
        </div>
    </t>
    </template>

    <template id="locasix_quote_report_2">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="locasix.locasix_quote_report_document" t-lang="doc.partner_id.lang"/>
            </t>
        </t>
    </template>
</odoo>