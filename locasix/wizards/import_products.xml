<odoo>
    <record id="import_product_wizard_form" model="ir.ui.view">
        <field name="name">locasix.product.import.form</field>
        <field name="model">locasix.product.import</field>
        <field name="arch" type="xml">
            <form string="Import des produits">
                <group>
                    <field name="file"/>
                </group>
                <footer>
                    <button name="import_products" type="object" string="Importer" class="oe_highlight"/>
                    <button special="cancel" string="Cancel"/>
                </footer>
            </form>

        </field>
    </record>
    
</odoo>